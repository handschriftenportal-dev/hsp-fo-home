{
  "name": "hsp-fo-home",
  "version": "1.7.2",
  "description": "hsp-fo-home",
  "license": "MIT",
  "files": [
    "dist/",
    "declaration/",
    "types.ts"
  ],
  "scripts": {
    "clean": "shx rm -rf ./node_modules ./dist ./test-reports ./target ./declaration *.tgz",
    "fix:prettier": "prettier --write \"./{src,test,server}/**/*.{css,md,js,jsx,json,ts,tsx}\"",
    "lint": "eslint './{src,test}/**/*'",
    "test": "jest --clearCache && jest",
    "test:light": "jest --clearCache && jest --coverage=false",
    "build:dev": "webpack --mode=development --watch",
    "build": "webpack --mode=production && tsc --project tsconfig.declaration.json",
    "start": "shx ln -sf ../index.html dist/index.html && node server/start.js",
    "deb": "echo not in use",
    "version": "echo $npm_package_version | tr -d '\n'",
    "artefactid": "echo $npm_package_name | tr -d '\n'",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/handschriftenportal-dev/hsp-fo-home.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.1",
    "@commitlint/config-conventional": "^17.4.0",
    "@fontsource/roboto": "^4.5.8",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.3",
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^11.2.7",
    "@types/css-mediaquery": "^0.1.1",
    "@types/express": "^4.17.14",
    "@types/http-proxy": "^1.17.9",
    "@types/jest": "^26.0.24",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^17.0.52",
    "@types/react-dom": "^17.0.18",
    "@types/react-redux": "^7.1.24",
    "@types/url-join": "^4.0.1",
    "@types/wpapi": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "clean-webpack-plugin": "^3.0.0",
    "clsx": "^1.2.1",
    "commitizen": "^4.2.6",
    "css-loader": "^6.7.2",
    "css-mediaquery": "^0.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.31.11",
    "express": "^4.18.2",
    "hsp-web-module": "1.30.0",
    "http-proxy": "^1.18.1",
    "husky": "^8.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.3.0",
    "msw": "^0.49.2",
    "node-fetch": "^2.6.7",
    "nuka-carousel": "^5.0.13",
    "prettier": "^2.8.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.9",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "shx": "^0.3.4",
    "style-loader": "^3.3.1",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.4.2",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.9.4",
    "url-join": "^2.0.5",
